Html <: Base {
  
macro
  := "tml-tr"
  
string
  := quotedString<"\"">
  
operator
  := "=" | ">" | "<"  
  
comment
  := startComment (~endComment any)* endComment
  
startComment
  = "<" space* "!" space* "--" space*

endComment
  = "--" space* ">"
  
token
  := macro | tagText | openTag | closeTag | literal | operator | namedToken
  
openTag
  = "<"
  
closeTag
  = ">"

tagText
  = closeTag (~openTag any)*


}