Objc <: Base {

macro
  := "TMLLocalizedString"
  | "TMLLocalizedAttributedString"
  | "TMLLocalizedDate"
  | "TMLLocalizedAttributedDate"

string
  := ((boxedString | cstring) space*)+
  
boxedString
  = "@" quotedString<"\"">
  
cstring
  = quotedString<"\""> 
  
nil
  := "nil"
  
bool
  += "YES" 
  | "NO" 
  | "@" bool --boxed
  
number
  += "@" number --boxed
  
runinOperator
  += "@" ("{" | "[") --boxed
  
openExpressionOperator
  += "@{" | "@(" | "@["

}